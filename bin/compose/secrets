#!/bin/sh
# bin/compose/secrets

rm "./secrets/pmd_mysql_password.txt"
printf "%s" "mysql_password" >> "./secrets/pmd_mysql_password.txt"

rm "./secrets/pmd_mysql_root_password.txt"
printf "%s" "mysql_root_password" >> "./secrets/pmd_mysql_root_password.txt"

rm "./secrets/pmd_mysql_user.txt"
printf "%s" "mysql_user" >> "./secrets/pmd_mysql_user.txt"